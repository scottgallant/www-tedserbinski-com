	{% for post in site.posts %}
	<entry>
		<title>{{ post.title | xml_escape }}</title>
		<link href="http://tedserbinski.com{{ post.url }}?utm_medium=feed&amp;utm_source=rss&amp;utm_campaign=link"/>
