layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Ted Serbinski - Entrepreneur & hacker turned early-stage VC living in Detroit</title>
	<link type="application/atom+xml" href="http://tedserbinski.com/atom.xml" rel="self"/>
 	<link type="text" href="http://tedserbinski.com" rel="alternate"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://tedserbinski.com</id>
	<author>
		<name>Ted Serbinski</name>
		<uri>http://tedserbinski.com/about.html</uri>
	</author>
	<rights>Copyright (c) 2006-{{ site.time | date(site.time, "%Y") }} Ted Serbinski. All rights reserved.</rights>
	
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title | xml_escape }}</title>
		<link href="http://tedserbinski.com{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://tedserbinski.com{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
