<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Ted Serbinski - Entrepreneur and open source hacker turned early stage VC rebuilding Detroit</title>
	<link type="application/atom+xml" href="http://tedserbinski.com/atom.xml" rel="self"/>
 	<link type="text" href="http://tedserbinski.com" rel="alternate"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://tedserbinski.com</id>
